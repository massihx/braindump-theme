{
	"name": "braindump",
	"publisher": "purple-vision",
	"displayName": "braindump",
	"description": "Braindump: Note Taking Syntax Highlighting",
	"version": "0.0.9",
	"icon": "images/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/massihx/braindump-theme.git"
	},
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Programming Languages",
		"Themes",
		"Formatters",
		"Other"
	],
	"contributes": {
		"languages": [
			{
				"id": "braindump",
				"aliases": [
					"braindump",
					"bd",
					"brain dump"
				],
				"extensions": [
					".bd"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "braindump",
				"scopeName": "source.bd",
				"path": "./syntaxes/braindump.tmLanguage.json"
			}
		],
		"themes": [
			{
				"label": "braindump",
				"uiTheme": "vs-dark",
				"path": "./themes/braindump-color-theme.json"
			}
		]
	},
	"galleryBanner": {
		"color": "#052529",
		"theme": "dark"
	},
	"scripts": {
		"build": "node --experimental-modules src/build.mjs",
		"release:major---": "npm run build && git add . && git commit -m ðŸ†•10.0.0ðŸ†• && npm version major & vsce publish && git push",
		"release:minor": "npm run build && git add . && git commit -m 10.2.0 && npm version minor & vsce publish && git push",
		"release:patch": "npm run build && git add . && git commit -m 10.2.1 && npm version patch & vsce publish && git push"
	},
	"dependencies": {},
	"devDependencies": {}
}
